		"mongodb": "^4.12.1",
		"multer": "^1.4.5-lts.1",
		"mysql": "^2.18.1",
		"mysql2": "^2.3.3",
		"nest-winston": "^1.8.0",
		"nestjs-ethers": "^1.0.3",
		"papaparse": "^5.3.2",
		"plaid": "^15.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.7",
		"swagger-ui-express": "^4.6.0",
