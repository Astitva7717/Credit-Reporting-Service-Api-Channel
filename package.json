{
	"name": "cryr-api-channel",
	"version": "0.0.1",
	"description": "CRYR Api Channel",
	"author": "sb",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build:qa": "NODE_ENV=qa nest build",
		"build:uat": "NODE_ENV=uat nest build",
		"build:prod": "NODE_ENV=prod nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start:build:qa": "NODE_ENV=qa node dist/src/main.js",
		"start:build:uat": "NODE_ENV=uat node dist/src/main.js",
		"start:build:prod": "NODE_ENV=prod node dist/src/main.js",
		"start": "NODE_ENV=dev nest start --debug --watch",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@blazity/nest-file-fastify": "1.0.0",
		"@fastify/cors": "8.3.0",
		"@fastify/formbody": "7.4.0",
		"@fastify/helmet": "11.0.0",
		"@fastify/multipart": "7.7.1",
		"@fastify/rate-limit": "8.0.1",
		"@fastify/static": "6.10.2",
		"@fastify/swagger": "7.6.1",
		"@liaoliaots/nestjs-redis": "^9.0.5",
		"@nestjs/axios": "^3.0.0",
		"@nestjs/common": "^10.0.5",
		"@nestjs/config": "^3.0.0",
		"@nestjs/core": "^10.0.5",
		"@nestjs/jwt": "^10.1.0",
		"@nestjs/microservices": "^10.0.5",
		"@nestjs/mongoose": "^10.0.0",
		"@nestjs/passport": "^10.0.0",
		"@nestjs/platform-express": "^10.0.5",
		"@nestjs/platform-fastify": "10.0.5",
		"@nestjs/schedule": "^3.0.1",
		"@nestjs/swagger": "^7.1.1",
		"@nestjs/typeorm": "^10.0.0",
		"@types/async-retry": "^1.4.5",
		"@types/cache-manager": "^4.0.2",
		"async-retry": "^1.3.3",
		"axios": "^1.2.0",
		"cache-manager": "^5.1.3",
		"cache-manager-redis-store": "^2.0.0",
		"cheerio": "^1.0.0-rc.12",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cross-env": "^7.0.3",
		"crypto": "^1.0.1",
		"date-fns": "^2.29.3",
		"elliptic": "^6.5.4",
		"fast-csv": "^4.3.6",
		"helmet": "^6.0.0",
